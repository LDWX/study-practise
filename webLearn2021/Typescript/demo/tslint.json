{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    },
    "rules": {
        "indent": [
            true,
            "spaces",
            4
        ],
        "interface-name": false,
        "no-consecutive-blank-lines": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "quotemark": [
            true,
            "single"
        ],
        "only-arrow-functions": false,
        "no-console": false,
        "interface-over-type-literal": false,
        "no-empty": false
    }
}