{
    "presets": [
      ["@babel/preset-env", {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }],
      "@babel/preset-react"
    ],
    "plugins":[
      ["@babel/plugin-proposal-decorators",{"legacy": true}],
      ["@babel/plugin-proposal-class-properties",{ "loose": true }]
    ]
  }